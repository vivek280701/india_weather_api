{
  "builds": [
    { "src": "weather_project/manage.py", "use": "@vercel/python" },
    { "src": "weather_project/staticfiles/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "/weather_project/staticfiles/$1" },
    { "src": "/(.*)", "dest": "weather_project/manage.py" }
  ]
}
